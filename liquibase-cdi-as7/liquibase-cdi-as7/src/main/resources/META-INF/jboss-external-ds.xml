<?xml version="1.0" encoding="UTF-8"?>
<datasources>
	<datasource enabled="true" jndi-name="${db.datasource}"
		pool-name="ArquillianEmbeddedMysqlPool">
		<connection-url>${db.url}</connection-url>
		<driver>mysql</driver>
		<security>
			<user-name>${db.username}</user-name>
			<password>${db.password}</password>
		</security>
	</datasource>
</datasources> 